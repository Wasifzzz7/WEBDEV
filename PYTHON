class Human:
    
    def __init__(self,Age,Height,Language):
        self.Age = Age
        self.Height = Height
        self.Language = Language


wasif = Human(12,130,"Bengali")
farhaan = Human(14,140,"Hindi")

print(wasif.Age)
print(farhaan.Age)

print(wasif.Height)
print(farhaan.Height)

class Animal:

    def __init__(self,sound):
        self.sound = sound


Dog = Animal("Bark,Bark")        
Cat = Animal("Meow,Meow")

print(Dog.sound)
print(Cat.sound)


class Car:

    def __init__(self,Model,Price):
        self.Model = Model
        self.Price = Price

def GetDetails(self):
    print("Car Model",self.Model)
    print("Car Price",self.Price)

def GetCarPrice(self):
    print("Car Price",self.Price)
