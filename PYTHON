#Data


Name = "Wasif" #String
Grade = 5 #Integer
Mathmarks = 97 #Float
PassesExam = True #Boolean DT
d = 6
c = 7

print(type(Name))
print(type(Grade))

#Integer Operation
a = 5
b = 4

#Numerical Operations
print(a+b)
print(a-b)
print(a*b)
print(a/b)
print(a**b)

#Logical Operation
print(a>b)
print(a<b)
print(a>=b)
print(a<=b)
print(a==b) #Equal
print(a!=b) #Not Equal
print(c+d)
print(d-c)
print(c-d)
print(d+c)
