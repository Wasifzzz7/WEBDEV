import turtle

screen = turtle.Screen()
screen.bgcolor("white")
screen.title("Lets draw with Wasif")


pen = turtle.Turtle()

# pen.fd(150)
# pen.left(90)
# pen.fd(100)
# pen.left(90)
# pen.fd(150)
# pen.left(90)
# pen.fd(100)
pen.pensize(3)
pen.color("red")
pen.speed(2)
pen.left(250)
pen.fd(100)



turtle.done()
